<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Process</title>
</head>
<body>
    <% data.Doctors.forEach(e => { %>
        <% if (e.Reservation.status === false && e.Reservation.prescription === null) { %>
            <h1>Sedang diperiksa</h1>
        <% } else { %>
            <p><%= e.Reservation.prescription %></p>
        <% } %>
    <% }); %>

    <a href="/patients/<%= data.id %>"> Back to list Doctor </a>
</body>
</html>